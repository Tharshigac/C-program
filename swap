#include <stdio.h>

int main(){
    int a=1,b=20;
    int*p=&a;
    int*q=&b;
    *p=*p+*q;
    *q=*p-*q;
    *p=*p-*q;
    printf("Ater swapping:%d %d",*p,*q);
}

